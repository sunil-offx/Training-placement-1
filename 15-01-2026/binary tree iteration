/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class BSTIterator {

    TreeNode dummy;
    TreeNode dummyPointer;

    public void inorder(TreeNode node){
        if(node == null){
            return;
        }

        inorder(node.left);
        dummyPointer.right = node;
        dummyPointer = dummyPointer.right;
        node.left = null;
        inorder(node.right);
    }

    public BSTIterator(TreeNode root) {
        
        dummy = new TreeNode(-1);
        dummyPointer = dummy; 
        inorder(root);
    }
    
    public int next() {
        if(dummy.right != null){
            dummy = dummy.right;
        }
        return dummy.val;
    }
    
    public boolean hasNext() {
        if(dummy.right == null){
            return false;
        }
        return true;
    }
}

/**
 * Your BSTIterator object will be instantiated and called as such:
 * BSTIterator obj = new BSTIterator(root);
 * int param_1 = obj.next();
 * boolean param_2 = obj.hasNext();
 */
