class Solution {
    public int countPrimes(int n) {
        int c=0;
        int ans[]=new int[n];
        markPrime(ans);
        for(int i=2;i<n;i++){
            if(ans[i]==0)c++;
        }
        return c;
    }

    public void markPrime(int a[]){
        int n=a.length;
        for(int i=2;i*i<n;i++){
            if(a[i]==1)continue;
            for(int j=2*i;j<n;j+=i){
                a[j]=1;
            }
        }
    }
}
