class Solution:
    def numDistinct(self, s: str, t: str) -> int:

        n,m=len(s)+1,len(t)+1
        dp=[0]*m
        curr=[0]*m
        dp[0]=1
        curr[0]=1
        for i in range(1,n):
            for j in range(1,m):
                if s[i-1]==t[j-1]:
                    curr[j]=dp[j-1]+dp[j]
                else:
                    curr[j]=dp[j]
            dp=curr[:]
        return dp[m-1]
